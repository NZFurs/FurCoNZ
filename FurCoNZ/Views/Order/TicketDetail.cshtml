@model IList<FurCoNZ.ViewModels.TicketDetailViewModel>

<form asp-action="Confirm" asp-controller="Order" method="post">
    <div class="accordion" id="attendee-details-accordion">
        @for (int i = 0; i < Model.Count; i++)
        {
            @Html.EditorFor(model => model[i])
        }
    </div>

    <button type="submit" class="btn btn-primary">Confirm Order &gt;</button>
</form>