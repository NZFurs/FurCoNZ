{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "files": [
        "jquery.slim.min.js",
        "jquery.slim.js"
      ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "popper.js@1.14.7",
      "files": [
        "esm/popper.min.js",
        "esm/popper.js"
      ],
      "destination": "wwwroot/lib/popper/"
    },
    {
        "provider": "unpkg",
        "library": "date-input-polyfill@2.14.0",
        "files": [
            "date-input-polyfill.dist.js"
        ],
        "destination": "wwwroot/lib/date-input-polyfill/"
    }
  ]
}