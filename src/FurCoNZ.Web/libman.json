{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validate@1.19.1",
      "files": [
        "jquery.validate.min.js",
        "jquery.validate.js",
        "additional-methods.min.js",
        "additional-methods.js"
      ],
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "popper.js@1.15.0",
      "files": [
        "esm/popper.min.js",
        "esm/popper.js"
      ],
      "destination": "wwwroot/lib/popper/"
    },
    {
        "provider": "unpkg",
        "library": "date-input-polyfill@2.14.0",
        "files": [
            "date-input-polyfill.dist.js"
        ],
        "destination": "wwwroot/lib/date-input-polyfill/"
    }
  ]
}